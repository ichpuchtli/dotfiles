#!/bin/sh
# Aliases

alias ls='ls --color=auto'
alias l='ls'
alias ll='ls -hl --group-directories-first'
alias la='ls -a'
alias lla='ls -hla'
alias grep='grep --color=auto'
alias del='rm -rf'
alias man='man -a'
alias svi='sudo vim'
alias git='logcmd hub'
alias vi='logcmd vim'
alias vim='logcmd vim'
alias gvim='logcmd gvim --nofork'
alias make='logcmd make'
